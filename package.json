{
  "name": "@sygnalgroup/react-sg-modules",
  "version": "1.2.2",
  "description": "Module to facilitate redux sagas and redux configurations with async request with expo",
  "author": "sygnalgroupbr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sygnalgroup/react-sg-modules.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "microbundle-crl": "^0.13.10",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.2",
    "prettier": "2.0.5"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/sygnalgroup/react-sg-modules/issues"
  },
  "homepage": "https://github.com/sygnalgroup/react-sg-modules#readme",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "history": "^4.10.1",
    "humps": "^2.0.1",
    "ramda": "^0.26.1",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "reduxsauce": "^1.1.0",
    "reselect": "^4.0.0",
    "seamless-immutable": "^7.1.4"
  },
  "keywords": [
    "redux",
    "redux-sagas",
    "axios",
    "modules"
  ]
}
